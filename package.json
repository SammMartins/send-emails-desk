{
  "name": "send-emails-desk",
  "version": "1.0.0",
  "description": "Um sistema de help-desk interno focado em eficiência, que cria tickets via e-mail. Inclui dashboards de desempenho com análise de SLA, IA para análise de texto, e gestão completa de tickets.",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "tsc && webpack --mode production",
    "build:main": "tsc",
    "build:renderer": "webpack --mode production",
    "dev": "NODE_ENV=development webpack serve --mode development",
    "start": "NODE_ENV=development npm run build:main && electron .",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "helpdesk",
    "email",
    "tickets",
    "customer-support",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "electron": "^38.2.1",
    "express": "^5.1.0",
    "googleapis": "^161.0.0",
    "openai": "^6.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
